generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model User {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  email     String  @unique
  name      String?
  password  String
  wins      Int     @default(0)
  losses    Int     @default(0)
  tie      Int     @default(0)
}